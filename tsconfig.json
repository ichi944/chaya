{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "react",
    "outDir": "./public/js",
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "lib": ["es2015", "es5", "dom", "scripthost", "dom.iterable"]
  },
  "include": [
      "resources/js/**/*"
  ]
}
